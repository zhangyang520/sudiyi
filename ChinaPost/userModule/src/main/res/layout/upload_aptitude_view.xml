<com.zhy.autolayout.AutoLinearLayout
        android:id="@+id/rl_portrait_licence"
        android:paddingLeft="20px"
        android:orientation="vertical"
        android:layout_marginTop="70px"
        android:layout_below="@+id/rl_food_licence"
